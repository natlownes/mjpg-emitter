{
  "name": "mjpg-emitter",
  "version": "0.0.2",
  "description": "throw jpegs into this and emit mjpgs after an amount of time you specify",
  "main": "lib/index.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/natlownes/mjpg-emitter.git"
  },
  "keywords": [
    "jpg",
    "mjpg",
    "video",
    "emitter",
    "secret eating"
  ],
  "directories": {
    "test": "test",
    "src": "src",
    "lib": "lib"
  },
  "scripts": {
